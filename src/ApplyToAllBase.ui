<!DOCTYPE UI><UI version="3.2" stdsetdef="1">
<class>ApplyToAllBase</class>
<widget class="QDialog">
    <property name="name">
        <cstring>ApplyToAllBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>481</width>
            <height>180</height>
        </rect>
    </property>
    <property name="caption">
        <string>confirm</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QPushButton" row="2" column="0">
            <property name="name">
                <cstring>PushButtonOk</cstring>
            </property>
            <property name="text">
                <string>Yes</string>
            </property>
        </widget>
        <widget class="QPushButton" row="2" column="1">
            <property name="name">
                <cstring>PushButtonCancel</cstring>
            </property>
            <property name="text">
                <string>No</string>
            </property>
        </widget>
        <widget class="QPushButton" row="2" column="2">
            <property name="name">
                <cstring>PushButtonAbort</cstring>
            </property>
            <property name="text">
                <string>Abort</string>
            </property>
        </widget>
        <widget class="QLabel" row="0" column="0" rowspan="1" colspan="3">
            <property name="name">
                <cstring>TextLabel</cstring>
            </property>
            <property name="text">
                <string></string>
            </property>
        </widget>
        <widget class="QCheckBox" row="1" column="0" rowspan="1" colspan="3">
            <property name="name">
                <cstring>CheckBoxApply</cstring>
            </property>
            <property name="text">
                <string>apply answer to all affected items</string>
            </property>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>PushButtonCancel</sender>
        <signal>clicked()</signal>
        <receiver>ApplyToAllBase</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>PushButtonOk</sender>
        <signal>clicked()</signal>
        <receiver>ApplyToAllBase</receiver>
        <slot>accept()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>CheckBoxApply</tabstop>
    <tabstop>PushButtonOk</tabstop>
    <tabstop>PushButtonCancel</tabstop>
    <tabstop>PushButtonAbort</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
</UI>

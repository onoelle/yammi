<!DOCTYPE UI><UI version="3.2" stdsetdef="1">
<class>UpdateDatabaseDialogBase</class>
<widget class="QDialog">
    <property name="name">
        <cstring>UpdateDatabaseDialogBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>433</width>
            <height>266</height>
        </rect>
    </property>
    <property name="caption">
        <string>Update Database - Scan Harddisk</string>
    </property>
    <property name="sizeGripEnabled">
        <bool>true</bool>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QPushButton" row="4" column="0">
            <property name="name">
                <cstring>PushButtonOk</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>Start Scan...</string>
            </property>
        </widget>
        <widget class="QPushButton" row="1" column="2">
            <property name="name">
                <cstring>ButtonChooseScanDir</cstring>
            </property>
            <property name="text">
                <string>...</string>
            </property>
            <property name="toolTip" stdset="0">
                <string>choose directory</string>
            </property>
        </widget>
        <widget class="QLineEdit" row="2" column="1">
            <property name="name">
                <cstring>LineEditFilePattern</cstring>
            </property>
            <property name="toolTip" stdset="0">
                <string>yammi will search in this directory (and recursive in all subdirectories) for new song files</string>
            </property>
        </widget>
        <widget class="QLineEdit" row="1" column="1">
            <property name="name">
                <cstring>LineEditScanDir</cstring>
            </property>
            <property name="toolTip" stdset="0">
                <string>yammi will search in this directory (and recursive in all subdirectories) for new song files</string>
            </property>
        </widget>
        <widget class="QLabel" row="1" column="0">
            <property name="name">
                <cstring>LabelScanDir</cstring>
            </property>
            <property name="text">
                <string>base directory for scanning</string>
            </property>
            <property name="toolTip" stdset="0">
                <string>yammi will search in this directory (and recursive in all subdirectories) for new song files</string>
            </property>
        </widget>
        <widget class="QLabel" row="0" column="0" rowspan="1" colspan="3">
            <property name="name">
                <cstring>TextLabel1</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>5</hsizetype>
                    <vsizetype>4</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="font">
                <font>
                    <bold>1</bold>
                    <italic>1</italic>
                </font>
            </property>
            <property name="text">
                <string>Options for scanning your harddisk</string>
            </property>
        </widget>
        <widget class="QLabel" row="2" column="0">
            <property name="name">
                <cstring>LabelFilePattern</cstring>
            </property>
            <property name="text">
                <string>pattern of files to import</string>
            </property>
            <property name="toolTip" stdset="0">
                <string>yammi will search in this directory (and recursive in all subdirectories) for new song files</string>
            </property>
        </widget>
        <widget class="QPushButton" row="4" column="1" rowspan="1" colspan="2">
            <property name="name">
                <cstring>PushButtonCancel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>Cancel</string>
            </property>
        </widget>
        <widget class="QCheckBox" row="3" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>CheckBoxFollowSymlinks</cstring>
            </property>
            <property name="enabled">
                <bool>false</bool>
            </property>
            <property name="text">
                <string>follow symbolic links</string>
            </property>
            <property name="toolTip" stdset="0">
                <string>Choose whether yammi should follow symbolic links when traversing the directory structure</string>
            </property>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>PushButtonOk</sender>
        <signal>clicked()</signal>
        <receiver>UpdateDatabaseDialogBase</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>PushButtonCancel</sender>
        <signal>clicked()</signal>
        <receiver>UpdateDatabaseDialogBase</receiver>
        <slot>reject()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>LineEditScanDir</tabstop>
    <tabstop>ButtonChooseScanDir</tabstop>
    <tabstop>LineEditFilePattern</tabstop>
    <tabstop>CheckBoxFollowSymlinks</tabstop>
    <tabstop>PushButtonOk</tabstop>
    <tabstop>PushButtonCancel</tabstop>
</tabstops>
<pixmapinproject/>
<layoutdefaults spacing="6" margin="11"/>
</UI>
